    "abort-controller": "^3.0.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "node-fetch": "^2.7.0",
    "node-telegram-bot-api": "^0.66.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "winston": "^3.14.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0"
