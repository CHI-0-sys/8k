    "backtest": "node scripts/run-backtest.js",
    "test": "jest",
    "lint": "eslint .",
    "logs": "tail -f logs/combined.log",
    "postinstall": "mkdir -p logs data"
  },
  "keywords": [
    "solana",
    "trading-bot",
    "cryptocurrency",
